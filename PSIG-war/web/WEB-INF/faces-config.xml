<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <managed-bean>
        <managed-bean-name>altaInmueble</managed-bean-name>
        <managed-bean-class>com.bean.AltaInmueble</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <managed-bean>
        <managed-bean-name>altaAdministrador</managed-bean-name>
        <managed-bean-class>com.bean.AltaAdministrador</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <from-view-id>ListarAdministrador.xhtml</from-view-id>
        <navigation-case>
                <from-outcome>EditarAdministrador</from-outcome>
                <to-view-id>EditarAdministrador.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>ListarAdministrador.xhtml</from-view-id>
        <navigation-case>
                <from-outcome>logout</from-outcome>
                <to-view-id>logout.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>EditarAdministrador.xhtml</from-view-id>
        <navigation-case>
                <from-outcome>logout</from-outcome>
                <to-view-id>logout.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>ListarAdministrador.xhtml</from-view-id>
        <navigation-case>
                <from-outcome>logout</from-outcome>
                <to-view-id>logout.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>login.xhtml</from-view-id>
        <navigation-case>
                <from-outcome>admin</from-outcome>
                <to-view-id>admin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>
